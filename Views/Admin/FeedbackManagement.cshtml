
@{
    ViewBag.Title = "FeedbackManagement";
    Layout = "~/Views/Shared/_AdminLayoutPage.cshtml";
}

<div class="header-two-title text-center" data-aos="fade-up">
    <p class="tagline">Welecome To Admin Feedback Mangagement</p>
</div>


<div class="col-xl-12 col-lg-8 col-md-12">
    <div class="row">
        <div class="col-md-12">
            <div class="settings-widget">
                <div class="settings-inner-blk p-0">
                    <div class="comman-space pb-0">
                        <div class="settings-tickets-blk course-instruct-blk table-responsive">

                            <table class="table table-nowrap mb-0" id="myTable">
                                <thead>
                                    <tr>
                                        <th>SR</th>
                                        <th>Name</th>
                                        <th>Email </th>
                                        <th>Course Title</th>
                                        <th>Subject </th>
                                        <th>Comment</th>
                                        <th>Status</th>
                                        <th>ACTION</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    @if (Model != null)
                                    {
                                        if (Model.Rows.Count > 0)
                                        {
                                            int i = 1;
                                            foreach (System.Data.DataRow row in Model.Rows)
                                            {
                                                <tr>
                                                    <td>@i</td>
                                                    <td>@row["fullname"]</td>
                                                    
                                                    <td>@row["email"]</td>
                                                    <td>@row["Course_Title"]</td>
                                                    <td>@row["subject"]</td>
                                                    <td>@row["comment"]</td>
                                                    <td>@row["status"]</td>
                                                    <td>
                                                        <a href="/admin/StudentManagement" onclick="return confirm('Do you really want to delete it?')">  <button class="btn btn-danger fa fa-trash" title="Delete"></button></a>
                                                        <button class="btn btn-success fa fa-pencil" title="Edit Record"></button>
                                                        <a href="/admin/StudentManagement" onclick="return confirm('Do you really want to change the status?')">     <button class="btn btn-primary fa fa-refresh" title="Change Status"></button></a>
                                                    </td>
                                                </tr>
                                                { i = i + 1; }
                                            }

                                        }

                                    }

                                </tbody>
                            </table>

                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<script src="~/Content/assets/js/jquery-3.6.0.min.js"></script>





